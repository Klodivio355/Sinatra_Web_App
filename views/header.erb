<!DOCTYPE html>
<html>
    
    <head>
        <% @cssURL.each do |record| %>
            <link rel="stylesheet" type="text/css" href="<%=h record %>">
        <% end %>
        <link rel="stylesheet" type="text/css" href="css/header.css">
        <% if @jsURL.size != 0 %>
            <% @jsURL.each do |record2| %>
                <script src="<%=h record2 %>"></script>
            <% end %>
            <meta http-equiv="refresh" content="300">
        <% end %>
        <title><%=h @title %></title>
    </head>
    
    <body>
         <header>
            <div>
                <ul class="nav-mask">
                    <li><a href="/home">Home</a></li>
                    <% if session[:logged_in] && session[:logged_isadmin] == 0 %>   
                    <li>
                        <a href="/accountInfo">Account Info</a>
                    </li>
                    <% end %>
                    <% if !session[:logged_in] %>
                    <li>
                        <a href="/login">Login</a>
                    </li>
                    <% else %>
                    <li>
                        <a href="/session_clear">Log Out</a>
                    </li>
                    <% end %>
                    <% if session[:logged_isadmin] == 1 || session[:logged_isadmin] == 2%> 
                        <li>
                            <a href="/admin_section">Administrator</a>
                        </li>
                        <li>
                            <a href="/admin_two">Database Updates</a>
                        </li>
                    <% end %>
                    <li>
                        <a href="/contact">Contact Us</a>
                    </li>
                </ul>
            </div>
         </header>